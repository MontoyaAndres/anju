{
  "name": "anju",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "git@github.com:MontoyaAndres/anju.git",
  "author": "MontoyaAndres <andresmontoyafcb@gmail.com>",
  "packageManager": "npm@11.3.0",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "clean": "turbo run clean && rm -r node_modules",
